<h3>商品一覧画面</h3>



<table>
	<thead>
	  <tr>
	    <th>商品ID</th>
	    <th>商品名</th>
	    <th>税抜価格</th>
	    <th>ジャンル</th>
	    <th>ステータス</th>
	  </tr>
	</thead>
    <% @products.each do |product| %>
	<tbody>
	  <tr>
	  	<td><%= product.id %></td>
	  	<td><%= link_to product.name, admin_product_path(product.id) %></td>
	  	<td><%= product.non_tax_price %></td>
	  	<td><%= product.genre_id %></td>
	  	<td><%= product.sales_status %></td>
	  </tr>
	</tbody>
	<% end %>
</table>